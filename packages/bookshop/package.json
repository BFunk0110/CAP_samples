{
  "name": "@sap/capire-bookshop",
  "version": "1.0.0",
  "description": "A simple bookshop application, build in a self-contained all-in-one fashion, i.e. w/o reusing other packages.",
  "repository": "https://github.wdf.sap.corp/caps/bookshop.git",
  "license": "ISC",
  "dependencies": {
    "@sap/cds": "latest",
    "express": "*"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql",
        "model": [
          "db",
          "srv"
        ]
      }
    }
  }
}
